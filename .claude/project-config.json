{
  "project": {
    "name": "Java Selenium Framework",
    "type": "automation-testing",
    "language": "java",
    "framework": "selenium-testng",
    "target_application": "https://demowebshop.tricentis.com/"
  },
  "build": {
    "tool": "maven",
    "java_version": "11",
    "commands": {
      "compile": "mvn clean compile",
      "test": "mvn clean test",
      "lint": "mvn checkstyle:check",
      "package": "mvn clean package"
    }
  },
  "testing": {
    "framework": "testng",
    "reporting": ["extentreports", "allure"],
    "parallel_execution": true,
    "browsers": ["chrome", "firefox", "edge"],
    "test_categories": ["smoke", "regression", "functional", "e2e"]
  },
  "environments": {
    "dev": "https://demowebshop.tricentis.com/",
    "staging": "https://demowebshop.tricentis.com/",
    "prod": "https://demowebshop.tricentis.com/"
  },
  "agents": {
    "framework_architect": {
      "role": "Main coordinator and architecture designer",
      "responsibilities": ["Overall framework design", "Coordination", "Code review"]
    },
    "page_object_creator": {
      "role": "Page Object Model implementation",
      "responsibilities": ["Create POM classes", "Element locators", "Page actions"]
    },
    "test_data_manager": {
      "role": "Data factories and test data management",
      "responsibilities": ["Dynamic data generation", "Test data files", "Data utilities"]
    },
    "config_manager": {
      "role": "Configuration and environment setup",
      "responsibilities": ["Environment configs", "Properties files", "Driver management"]
    },
    "test_creator": {
      "role": "Test case implementation",
      "responsibilities": ["Test scenarios", "Test utilities", "Assertions"]
    },
    "reporting_specialist": {
      "role": "Reporting and logging setup",
      "responsibilities": ["Report configuration", "Logging setup", "Screenshots"]
    }
  }
}