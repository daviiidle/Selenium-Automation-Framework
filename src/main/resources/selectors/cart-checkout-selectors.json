  "test_scenarios": {
    "cart_management": {
      "add_items_to_cart": {
        "steps": [
          "Navigate to product page",
          "Select quantity",
          "Click add to cart",
          "Verify cart update"
        ],
        "assertions": [
          "Cart item count increases",
          "Cart total updates",
          "Product appears in cart"
        ]
      },
      "update_cart_quantities": {
        "steps": [
          "Go to cart page",
          "Change item quantity",
          "Click update cart",
          "Verify changes"
        ],
        "assertions": [
          "Quantity updates correctly",
          "Subtotal recalculates",
          "Total updates"
        ]
      },
      "remove_items": {
        "steps": [
          "Go to cart page",
          "Click remove item",
          "Verify removal"
        ],
        "assertions": [
          "Item removed from cart",
          "Cart total updates",
          "Empty cart message if no items"
        ]
      }
    },
    "checkout_flow": {
      "guest_checkout": {
        "steps": [
          "Add items to cart",
          "Go to checkout",
          "Choose guest checkout",
          "Fill billing address",
          "Select shipping method",
          "Choose payment method",
          "Confirm order"
        ],
        "assertions": [
          "Order processes successfully",
          "Order confirmation displayed",
          "Order number generated"
        ]
      },
      "registered_user_checkout": {
        "steps": [
          "Login as registered user",
          "Add items to cart",
          "Go to checkout",
          "Select saved address or add new",
          "Complete checkout process"
        ],
        "assertions": [
          "Saved addresses available",
          "User information pre-filled",
          "Order completes successfully"
        ]
      }
    }
