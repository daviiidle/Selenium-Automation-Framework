  "notes": {
    "selector_stability": {
      "High": "Stable selectors using IDs or semantic attributes, unlikely to change",
      "Medium": "Class-based selectors that may change with styling updates",
      "Low": "Brittle selectors that may break with UI changes"
    },
    "automation_recommendations": [
      "Prefer ID-based selectors when available",
      "Use data attributes if the application supports them",
      "Implement fallback selector strategies",
